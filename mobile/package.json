    "react-native-dialogs": "^1.1.0",
    "react-native-dotenv": "^0.2.0",
    "react-native-geolocation-service": "^4.0.2",
    "react-native-maps": "0.27.1",
    "react-relay": "^0.0.0-experimental-895a6fe0",
    "relay-compiler-language-typescript": "^12.0.3",
    "relay-runtime": "^9.1.0",
    "subscriptions-transport-ws": "^0.9.16"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.36",
    "@types/react-native": "^0.62.0",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-relay": "^7.0.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "16.9.2",
    "@types/relay-runtime": "^8.0.8",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-relay": "^9.1.0",
    "eslint": "^6.5.1",
    "graphql": "^15.0.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "16.11.0",
    "relay-compiler": "^9.1.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
